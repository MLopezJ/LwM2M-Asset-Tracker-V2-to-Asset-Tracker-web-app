{
  "name": "@nordicsemiconductor/asset-tracker-lwm2m-js",
  "version": "0.0.0-development",
  "description": "Converts a JSON document containing the asset_tracker_v2 device and sensor data encoded as LwM2M to the JSON document required by nRF Asset Tracker.",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "husky install",
    "prepublish": "npx swc -d dist ./src"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-lwm2m-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-lwm2m-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-lwm2m-js#readme",
  "keywords": [
    "lwm2m",
    "asset-tracker-v2",
    "nrf-asset-tracker"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-docs": "29.0.2",
    "@nordicsemiconductor/lwm2m-types": "2.5.1",
    "@sinclair/typebox": "0.30.4"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.68",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "16.0.11",
    "@swc/cli": "0.1.62",
    "@swc/jest": "0.2.28",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "prettier": "3.0.1",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  }
}
