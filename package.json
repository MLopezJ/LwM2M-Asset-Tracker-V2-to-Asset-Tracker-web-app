{
  "name": "asset-tracker-lwm2m",
  "version": "1.0.0",
  "description": "Convert the LwM2M Asset Tracker v2 JSON encoding to Asset Tracker web app JSON encoding",
  "main": "index.js",
  "scripts": {
    "test": "find ./ -type f -name '*.spec.ts' ! -path '*/node_modules/*' -exec npx tsx {} ';'",
    "prepare": "husky install",
    "prepublish": "npx swc -d dist ./src"
  },
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./export.js",
      "node": "./export.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MLopezJ/sset-tracker-lwm2m-js.git"
  },
  "bugs": {
    "url": "https://github.com/MLopezJ/sset-tracker-lwm2m-js/issues"
  },
  "homepage": "https://github.com/MLopezJ/sset-tracker-lwm2m-js#readme",
  "keywords": [
    "lwm2m",
    "asset-tracker-v2",
    "nrf-asset-tracker"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-docs": "29.0.2",
    "@nordicsemiconductor/lwm2m-types": "2.5.1",
    "hamjest": "4.0.1"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.68",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "16.0.11",
    "@swc/cli": "0.1.62",
    "@types/node": "20.5.0",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "husky": "8.0.3",
    "prettier": "3.0.1",
    "semantic-release": "21.1.1",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  }
}
