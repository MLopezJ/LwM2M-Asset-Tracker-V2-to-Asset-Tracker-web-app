{
  "name": "lwm2m-asset-tracker-v2-to-asset-tracker-web-app",
  "version": "1.0.0",
  "description": "Convert the LwM2M Asset Tracker v2 JSON encoding to Asset Tracker web app JSON encoding",
  "main": "index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "husky install",
    "prepublish": "npx swc -d dist ./src"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MLopezJ/LwM2M-Asset-Tracker-V2-to-Asset-Tracker-web-app.git"
  },
  "bugs": {
    "url": "https://github.com/MLopezJ/LwM2M-Asset-Tracker-V2-to-Asset-Tracker-web-app/issues"
  },
  "homepage": "https://github.com/MLopezJ/LwM2M-Asset-Tracker-V2-to-Asset-Tracker-web-app#readme",
  "keywords": [
    "LwM2M Asset Tracker v2",
    "Asset Tracker Web App"
  ],
  "author": "Nordic Semiconductor",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-docs": "29.0.2",
    "@nordicsemiconductor/lwm2m-types": "2.5.1",
    "hamjest": "4.0.1"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.68",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "16.0.11",
    "@swc/cli": "0.1.62",
    "@swc/jest": "0.2.28",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "prettier": "3.0.1",
    "tsx": "3.12.7",
    "typescript": "5.1.6"
  }
}
